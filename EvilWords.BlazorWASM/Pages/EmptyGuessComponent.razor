@for (var charResultIndex = 0; charResultIndex < WordLength; charResultIndex++)
{
    <MudChip
        Color="Color.Dark"
        Variant="Variant.Filled"
        Label="true" Size="Size"/>
    
}

@code {
    [Parameter]
    [EditorRequired]
    public int WordLength { get; set; }
    

    [Parameter]
    [EditorRequired]
    public Size Size { get; set; }
}
